{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-mvc": "^3.0",
        "zfcampus/zf-development-mode": "^3.0",
        "zendframework/zend-cache": "^2.7",
        "zendframework/zend-db": "^2.8",
        "zendframework/zend-mvc-form": "^1.0",
        "zendframework/zend-json": "^3.0",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-mvc-console": "^1.1",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-mvc-plugins": "^1.0.1",
        "zendframework/zend-psr7bridge": "^0.2.2",
        "zendframework/zend-session": "^2.7",
        "zendframework/zend-servicemanager-di": "^1.1",
        "zendframework/zend-navigation": "^2.8",
        "zendframework/zend-paginator": "^2.7",
        "zendframework/zend-barcode": "^2.6",
        "zendframework/zend-captcha": "^2.7",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-http": "^2.6",
        "doctrine/doctrine-orm-module": "dev-master",
        "zendframework/zend-developer-tools": "dev-master",
        "beberlei/DoctrineExtensions": "^1.0",
        "oro/doctrine-extensions": "^1.1",
        "jms/serializer": "^1.6",
        "zf-commons/zfc-admin": "^0.3.1",
        "zendframework/zend-validator": "^2.9",
        "zendframework/zend-code": "^3.1",
        "zendframework/zend-crypt": "^3.2",
        "zendframework/zend-console": "^2.6",
        "zendframework/zend-debug": "^2.5",
        "zendframework/zend-diactoros": "^1.3",
        "psr/http-message": "^1.0",
        "zendframework/zend-hydrator": "^2.2",
        "zendframework/zend-di": "^2.6",
        "zendframework/zend-dom": "^2.6",
        "zendframework/zend-escaper": "^2.5",
        "zendframework/zend-file": "^2.7",
        "zendframework/zend-progressbar": "^2.5",
        "zendframework/zend-eventmanager": "^3.1",
        "zendframework/zend-inputfilter": "^2.7",
        "zendframework/zend-json-server": "^3.0",
        "zendframework/zend-xml2json": "^3.0",
        "firephp/firephp-core": "^0.4.0",
        "ccampbell/chromephp": "^4.1",
        "zendframework/zend-loader": "^2.5",
        "zendframework/zend-math": "^3.0",
        "zendframework/zend-mail": "^2.7",
        "zendframework/zend-memory": "^2.5",
        "zendframework/zend-mime": "^2.6",
        "zendframework/zend-modulemanager": "^2.7",
        "zendframework/zend-mvc-plugin-fileprg": "^1.0",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
        "zendframework/zend-mvc-plugin-identity": "^1.0",
        "zendframework/zend-mvc-plugin-prg": "^1.0",
        "zendframework/zend-permissions-acl": "^2.6",
        "zendframework/zend-permissions-rbac": "^2.5",
        "zendframework/zend-serializer": "^2.8",
        "zendframework/zend-server": "^2.7",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-stdlib": "^3.1",
        "zendframework/zend-soap": "^2.6",
        "zendframework/zend-tag": "^2.6",
        "zendframework/zend-test": "^3.0",
        "zendframework/zend-text": "^2.6",
        "zendframework/zend-uri": "^2.5",
        "zendframework/zend-i18n-resources": "^2.5",
        "zendframework/zend-view": "^2.9",
        "zendframework/zend-xmlrpc": "^2.6",
        "zfcampus/zf-console": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
    },
    "require-dev": {
        "zendframework/zend-component-installer": "^0.7.0"
    }
}
